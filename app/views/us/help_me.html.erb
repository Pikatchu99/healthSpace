<style>
  .fullwidth-container {
      display: block;
      width: 80%;
      margin: 2rem 10%
  }
  
  
  .line-heading {
      font-family: sans-serif;
      color: #60c0ba;
      font-weight: 600;
      line-height: 1.1;
      align-items: center;
      width: 90%;
      margin: 10px 0 0px 10%;
      white-space: nowrap;
      font-size: 26px;
      text-shadow: 1px 1px 1px;
  }
  
  #faqSection div {
      float: left;
      display: inline-block;
      width: 46%;
      margin: 0 2%;
      overflow: hidden;
  }
  
  #faqSection .collapsible {
      background-color: #fff;
      border: 1px solid hsla(0, 0%, 51.4%, .16);
      border-radius: 7px;
      box-shadow: 0 10px 30px -15px;
      transition: box-shadow .2s ease 0s;
      color: #60c0ba;
      margin: 0 0 8px;
      width: 100%;
      padding: 12px;
      font-family: sans-serif;
      font-size: 15px;
      text-align: left;
  }
  
  #faqSection .collapsible:hover {
      box-shadow: 0 10px 60px -15px;
  }
  
  #faqSection .active,
  #faqSection .collapsible:hover {}
  
  #faqSection .collapsible:after {
      content: "\002B";
      font-weight: bold;
      float: right;
      margin-left: 5px
  }
  
  #faqSection .active:after {
      content: "\2212"
  }
  
  #faqSection .faq-content {
      overflow: hidden;
      width: calc(100% - 8%);
      padding: 5px 2%;
      transition: all .25s ease-in-out 0s;
      background: #60c0ba;
      border: 1px solid hsla(0, 0%, 51.4%, .16);
      border-radius: 7px;
      max-height: 0px;
      visibility: hidden;
  }
  
  #faqSection .faq-content p {
      font-family: sans-serif;
      color: #fff;
      line-height: 140%;
      font-size: 14px;
      margin: 0 0 12px;
      text-shadow: 1px 1px 0 #2480;
      font-weight: 100;
  }
  
  #faqSection .faq-content a {
      color: #00C58E;
      font-weight: 600;
      letter-spacing: .05rem;
  }
</style>
